{
  "name": "live-session-hub",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/working-with-someone/live-session-hub.git",
  "author": "seungho-hub <63999413+kangseungho-hub@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "watch": "dotenvx run --env-file=.env.development -- nodemon",
    "compile": "tsc",
    "local": "dotenvx run --env-file=.env.local -- nodemon",
    "start": "node dist/server.js",
    "lint": "eslint . --config eslint.config.js",
    "db:migrate:prod": "dotenvx run --env-file=.env -- prisma migrate deploy",
    "db:migrate:dev": "dotenvx run --env-file=.env.development -- prisma migrate dev",
    "db:migrate:test": "dotenvx run --env-file=.env.test -- prisma migrate dev",
    "db:push:dev": "dotenvx run --env-file=.env.development -- prisma db push",
    "db:push:test": "dotenvx run --env-file=.env.test -- prisma db push",
    "db:reset:dev": "dotenvx run --env-file=.env.development -- prisma migrate reset",
    "db:reset:test": "dotenvx run --env-file=.env.test -- prisma migrate reset",
    "test": "dotenvx run --env-file=.env.test -- yarn run test:unit && yarn run test:integration",
    "test:unit": "dotenvx run --env-file=.env.test -- jest tests/unit --config=jest.config.unit.js",
    "test:integration": "dotenvx run --env-file=.env.test -- jest tests/integration --runInBand --config=jest.config.integration.js",
    "test:single": "dotenvx run --env-file=.env.test -- jest --runInBand --config=jest.config.integration.js"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/express-session": "^1.18.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.14",
    "@types/redis": "^4.0.11",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.9",
    "prettier": "^3.7.4",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.50.1"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.32.1",
    "@prisma/client": "^6.2.1",
    "@types/supertest": "^6.0.3",
    "connect-redis": "9.0.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "express-session": "^1.18.1",
    "heap-js": "^2.7.1",
    "http-status-codes": "^2.3.0",
    "node-cron": "^4.2.1",
    "prisma": "^6.2.1",
    "socket.io": "^4.8.1"
  }
}
